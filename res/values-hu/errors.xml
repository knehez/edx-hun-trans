<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Network error messages -->
    <!-- Error shown when  user attempts an action but needs an internet connection -->
    <string name="network_not_connected">Nem csatlakoztál az internethez.</string>
    <!-- Concise error shown when user attempts an action but needs an internet connection -->
    <string name="network_not_connected_short">Hálózat nem elérhető</string>
    <!-- Alert dialog title shown when a network request fails -->
    <string name="reset_no_network_title">Kapcsolódási hiba</string>
    <!-- Alert dialog content shown when a network request fails because the user has no internet connection -->
    <string name="reset_no_network_message">Nem csatlakoztál az internethez. Ellenőrizd az internet kapcsolatod.</string>
    <!-- Title of prompt indicating issue loading a video -->
    <string name="network_error_header">Hiba a videó betöltése közben</string>
    <!-- Content of prompt indicating issue loading a video -->
    <string name="network_error_message">Ellenőrizd az internet kapcsolatod.</string>

    <!-- Login error messages -->
    <!-- Error shown during login when user attempts login without entering a username or e-mail -->
    <string name="error_enter_email">Add meg a felhasználói neved vagy e-mail címed és próbáld újra.</string>
    <!-- Error shown during login when user enters an invalid email address -->
    <string name="error_invalid_email">Ellenőrizd, hogy az e-mail címed helyes és próbáld újra.</string>
    <!-- Error shown during login when user attempts login without entering a password -->
    <string name="error_enter_password">Add meg a jelszavad és próbáld újra.</string>
    <!-- Title of alert shown during login when user attempts login via Facebook without having linked their account -->
    <string name="error_account_not_linked_title_fb">A Facebook fiók nem kapcsolódik a MeMooc fiókhoz</string>
    <!-- Content of alert shown during login when user attempts login via Facebook without having linked their account -->
    <string name="error_account_not_linked_desc_fb">Ellenőrizd, hogy a Facebook fiókod össze van rendelve a MeMooc fiókoddal</string>
    <!-- Title of alert shown during login when user attempts login via Google without having linked their account -->
    <string name="error_account_not_linked_title_google">A Google fiók nem kapcsolódik a MeMooc fiókhoz</string>
    <!-- Content of alert shown during login when user attempts login via Google without having linked their account -->
    <string name="error_account_not_linked_desc_google">Ellenőrizd, hogy a Google fiókod össze van rendelve a MeMooc fiókoddal</string>
    
    <!-- Download error messages -->
    <!-- Label shown on video list item whose download failed -->
    <string name="error_download_failed">Letöltési hiba</string>
    <!-- Confirmation message shown when attempting to download a large quantity of video data -->
    <string name="file_size_exceeded">A letöltendő fájlok mérete nagyobb, mint a rendelkezésre álló szabad terület.</string>
    <!-- Confirmation message shown when attempting to download a large quantity of video data -->
    <string name="download_exceed_message">A kiválsztott videók nagyobbak, mint 1 GB. 
        Le akarod tölteni a videókat?</string>
    
    <!-- Message shown when user attempts to download a video over cellular data,
    but has chosen to only allow video downloads over wifi -->
    <string name="wifi_off_message">A letöltési beállítások csak Wi-Fi letöltést engednek. 
        \n Csatlakozz egy Wi-Fi hálózathoz, vagy módosítsd a letöltési beállításokat.</string>
    
    <!-- Offline Access -->
    <!-- Label shown when user attempts to view a course section that hasn't been synced -->
    <string name="section_offline_header">Ez a fejezet offline módban nem áll rendelkezésre</string>
    <!-- Label shown when user attempts to view a course section that hasn't been synced -->
    <string name="section_offline_message">Ha egy másik fejezethet töltöttél le tartalmat, 
        \nakkor menj ahhoz a fejezethez, hogy megtekinthesd.</string>
    <!-- Alert dialog title shown when user attempts to view a video that hasn't been synced -->
    <string name="video_offline_header">Ez a videó offline módban nem áll rendelkezésre.</string>
    <!-- Alert dialog content shown when user attempts to view a video that hasn't been synced -->
    <string name="video_offline_message">Válaszd ki a videót, amelyet letöltöttél.</string>

    <!-- Registration Error Messages -->

    <!-- Error prompt shown during registration to indicate an empty field. Argument is a field name -->
    <string name="error_enter_field">Add meg a %1$s.</string>
    <!-- Error prompt shown during registration to indicate a field missing a selection. Argument is a field name -->
    <string name="error_select_field">Válaszd ki a %1$s.</string>
    <!-- Error prompt shown during registration when a field entry is too short. Arguments are 1) a field name, 2) a number -->
    <string name="error_min_length">%1$s legalább %2$d karakter hosszú legyen.</string>
    <!-- Error prompt shown during registration when a field entry is too long. Arguments are 1) a field name, 2) a number -->
    <string name="error_max_length">%1$s legfeljebb %2$d karakter hosszú legyen.</string>

</resources>
